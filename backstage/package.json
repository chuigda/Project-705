{
  "name": "cnpr",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "launch": "ts-node -r tsconfig-paths/register src/index.ts",
    "launch_debug": "DEBUG=1 ts-node -r tsconfig-paths/register src/index.ts",
    "release_build": "tsc --project tsconfig.build.json && resolve-tspaths --project tsconfig.build.json",
    "make_dts": "npm-run-all make_dts:*",
    "make_dts:game_context": "dts-bundle-generator src/executor/game_context.ts -o game_context.d.ts",
    "make_dts:ruleset": "dts-bundle-generator src/ruleset/index.ts -o ruleset.d.ts",
    "clean": "npm-run-all clean:*",
    "clean:dts": "rm -f *.d.ts",
    "clean:dist": "rm -rf dist",

    "test_quick_ts": "ts-node -r tsconfig-paths/register test/test_quick.ts",
    "test_type_assert": "ts-node -r tsconfig-paths/register test/test_type_assert.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "dts-bundle-generator": "^6.12.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "json5": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "resolve-tspaths": "^0.7.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "express": "^4.18.1",
    "uuid": "^8.3.2"
  }
}
